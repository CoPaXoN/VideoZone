<UserControl
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk" x:Class="VideoZoneV2.SharedVideos"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">

    <Grid x:Name="LayoutRoot" Background="White" Margin="0,0,-67,0">

        <sdk:DataGrid x:Name="dgSharedVideos" HorizontalAlignment="Left" Height="126" Margin="10,104,0,0" VerticalAlignment="Top" Width="447"/>
        <Image x:Name="subject" HorizontalAlignment="Left" Height="64" Margin="10,10,-71,0" VerticalAlignment="Top" Width="461" Stretch="None" Source="Images/SharedVideos.png"/>
        <Image x:Name="Watch" MouseLeftButtonUp="Button_Click"  HorizontalAlignment="Left" Height="59" Margin="10,235,0,0" VerticalAlignment="Top" Width="103" Stretch="None" Source="Images/Wacth.png" />
        <Image x:Name="Back" MouseLeftButtonUp="Button_Click_1" HorizontalAlignment="Left" Height="59" Margin="360,235,0,0" VerticalAlignment="Top" Width="107" Stretch="None" Source="Images/Back.png" />

    </Grid>
</UserControl>
